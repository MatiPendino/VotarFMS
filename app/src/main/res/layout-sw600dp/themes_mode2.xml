        android:layout_marginTop="50dp"
        android:text="Segundo MC"
        android:textStyle="bold"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_firstMcThemes"
        android:layout_below="@+id/et_fm1"
        android:layout_alignParentStart="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="70dp"
        android:text="Primer MC"
        android:textStyle="bold"
        android:textSize="@dimen/mcs_names_tablet"/>


    <Button
        android:id="@+id/et_fm1"
        android:layout_below="@+id/text_secondMcThemes"
        android:layout_alignParentStart="true"
        android:layout_width="@dimen/width_point_tablet"
        android:layout_height="@dimen/height_point_tablet"
        android:textSize="@dimen/text_size_points_tablet"
        android:layout_marginTop="@dimen/margin_top_point_tablet"
        android:layout_marginStart="50dp"
        android:background="@color/buttonPoints"
        android:ems="10"
        android:textColor="#fff"
        android:inputType="numberDecimal"
        android:textAlignment="center" />

    <Button
        android:id="@+id/et_fm2"
        android:background="@color/buttonPoints"
        android:textAlignment="center"
        android:textColor="#fff"
        android:layout_width="@dimen/width_point_tablet"
        android:layout_height="@dimen/height_point_tablet"
        android:textSize="@dimen/text_size_points_tablet"
        android:layout_marginTop="@dimen/margin_top_point_tablet"
        android:layout_marginStart="@dimen/margin_between_points_tablet"
        android:layout_below="@+id/text_secondMcThemes"
        android:layout_toEndOf="@+id/et_fm1"
        android:ems="10"
        android:inputType="numberDecimal" />

    <Button
