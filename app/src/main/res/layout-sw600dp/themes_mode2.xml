     android:textSize="@dimen/text_extra_points_tablet"
     android:text="@string/escena" />

    <TextView
        android:id="@+id/txt_flow"
        android:layout_width="wrap_content"
        android:layout_marginStart="@dimen/margin_between_points_tablet"
        android:layout_marginTop="@dimen/top_texts_extra_points_tablet"
        android:textSize="@dimen/text_extra_points_tablet"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/txt_scene"
        android:text="@string/flow" />

    <TextView
        android:id="@+id/txt_skills"
        android:layout_width="wrap_content"
        android:layout_marginStart="@dimen/margin_between_points_tablet"
        android:layout_marginTop="@dimen/top_texts_extra_points_tablet"
        android:textSize="@dimen/text_extra_points_tablet"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/txt_flow"
        android:text="@string/skills" />

    <Button
        android:id="@+id/et_fmScene"
        android:layout_below="@+id/txt_scene"
        android:layout_marginStart="190dp"
        android:layout_toEndOf="@+id/et_fm4"
        android:background="@color/buttonPoints"
        android:textSize="@dimen/text_size_points_tablet"
        android:layout_width="@dimen/width_point_tablet"
        android:layout_height="@dimen/height_point_tablet"
        android:layout_marginTop="@dimen/top_extra_points_tablet"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textAlignment="center"
        android:textColor="#fff" />

    <Button
        android:id="@+id/et_fmFlow"
        android:background="@color/buttonPoints"
        android:textColor="#fff"
        android:textSize="@dimen/text_size_points_tablet"
        android:layout_width="@dimen/width_point_tablet"
        android:layout_height="@dimen/height_point_tablet"
        android:layout_marginTop="@dimen/top_extra_points_tablet"
        android:layout_marginStart="@dimen/between_extra_points_tablet"
        android:textAlignment="center"
        android:layout_below="@+id/txt_flow"
        android:layout_toEndOf="@+id/et_fmScene"
        android:ems="10"
        android:inputType="numberDecimal" />

    <Button
        android:id="@+id/et_fmSkills"
        android:background="@color/buttonPoints"
        android:textColor="#fff"
        android:textSize="@dimen/text_size_points_tablet"
        android:layout_width="@dimen/width_point_tablet"
        android:layout_height="@dimen/height_point_tablet"
        android:layout_marginTop="@dimen/top_extra_points_tablet"
        android:layout_marginStart="@dimen/between_extra_points_tablet"
        android:textAlignment="center"
        android:layout_below="@+id/txt_skills"
        android:layout_toEndOf="@+id/et_fmFlow"
        android:ems="10"
        android:inputType="numberDecimal" />

 <TextView
