     android:layout_marginTop="71dp"
     android:text="@string/escena" />

 <TextView
        android:id="@+id/txt_flow2"
        android:layout_width="wrap_content"
        android:layout_marginStart="@dimen/margin_between_points_tablet"
        android:layout_marginTop="71dp"
         android:textSize="@dimen/text_extra_points_tablet"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_fmFlow"
        android:layout_toEndOf="@+id/txt_scene2"
